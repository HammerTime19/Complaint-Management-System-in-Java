        jTextField3.setText(selectedValue);
    }                                          

    private void jComboBox3ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        String selectedValue = jComboBox3.getSelectedItem().toString();
        jTextField4.setText(selectedValue);
    }                                          

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        if(jTextField1.getText().equals("")){
            JOptionPane.showMessageDialog(null,"Please enter full name");
        }
        else if(jTextField2.getText().equals("")){
            JOptionPane.showMessageDialog(null,"Please enter your course");
        }
             else if(jTextField3.getText().equals("")){
            JOptionPane.showMessageDialog(null,"Please select your year");
             }
            else if(jTextField4.getText().equals("")){
            JOptionPane.showMessageDialog(null,"Please select your division");
    }         
            else if(jTextField5.getText().equals("")){
            JOptionPane.showMessageDialog(null,"Please enter your complaint");
    }   
            else {
                JOptionPane.showMessageDialog(null,"Successfully Sent");
            }
        
        String msg=jTextField5.getText();
        new Admin(msg).setVisible(true);
        setVisible(false);
    }                                        

    private void jTextField5ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        new Login().setVisible(true);
            this.setVisible(false);
    }                                        

